{
    "name": "steerpike/machinetags",
    "description": "Provides Machine Tag trait functionality",
    "authors": [
        {
            "name": "Steerpike",
            "email": "drop.table.people@googlemail.com"
        }
    ],
    "require": {
    	"php": "^7.0",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Steerpike\\MachineTags\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Steerpike\\MachineTags\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Steerpike\\MachineTags\\MachineTagsServiceProvider"
            ]
        }
    }
}
